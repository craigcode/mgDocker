ARG APPSERVER_PARENT_IMAGE
ARG XPA_DEV_IMG_VERSION

FROM ${APPSERVER_PARENT_IMAGE}:${XPA_DEV_IMG_VERSION}

ARG ADDITIONAL_INI
ARG LICENSE_FILE

ENV ADDITIONAL_INI=${ADDITIONAL_INI}
ENV LICENSE_FILE=${LICENSE_FILE}

USER ${USER}

COPY ${LICENSE_FILE} .
COPY ${ADDITIONAL_INI} .

#CMD sleep infinity
ENTRYPOINT entrypoint.sh    # entrypoint.sh exists in base image
